{
  "name": "nodelist",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.0",
    "grunt-bower-install-simple": "^1.1.3",
    "grunt-browser-sync": "^2.1.2",
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.13.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "^0.12.2",
    "grunt-contrib-htmlmin": "^0.4.0",
    "grunt-contrib-imagemin": "^0.9.3",
    "grunt-contrib-uglify": "^0.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^17.0.0",
    "grunt-filerev": "^2.2.0",
    "grunt-newer": "^1.1.0",
    "grunt-postcss": "^0.6.0",
    "grunt-sass": "^1.0.0",
    "grunt-svgmin": "^2.0.1",
    "grunt-text-replace": "^0.4.0",
    "grunt-usemin": "^3.0.0",
    "grunt-wiredep": "^2.0.0",
    "jit-grunt": "^0.9.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "export PATH=\"node_modules/.bin:$PATH\" ; test -d node_modules || npm install ; command -v grunt >/dev/null 2>&1 || npm install grunt-cli ; grunt",
    "serve:dev": "export PATH=\"node_modules/.bin:$PATH\" ; test -d node_modules || npm install ; command -v grunt >/dev/null 2>&1 || npm install grunt-cli ; grunt serve:dev",
    "serve:dist": "export PATH=\"node_modules/.bin:$PATH\" ; test -d node_modules || npm install ; command -v grunt >/dev/null 2>&1 || npm install grunt-cli ; grunt serve:dist"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "mocha": true,
      "jquery": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "indent": [
        2,
        "tab"
      ],
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-unused-vars": [
        2,
        {
          "args": "none"
        }
      ]
    },
    "globals": {
      "moment": true,
      "w2ui": true,
      "w2popup": true,
      "subRecords": true,
      "nodelistconfig": true
    }
  },
  "dependencies": {}
}
